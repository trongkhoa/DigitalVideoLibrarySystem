<title>Add Customer</title>
<script>
	$(document).ready(function() {
		var availableFName = new Array();
		var availableLName = new Array();
		var availableAddress = new Array();
		var availableCity = new Array();
		var availableZipCode = new Array();
		var availableMemNumb = new Array();
		$.get("DisplayAllMembersJSON",

		function(data, status) {
			if (data) {

				data.forEach(function(datum) {
					if (datum.membershipno) {
						availableFName.push(datum.fname);
						availableLName.push(datum.lname);
						availableAddress.push(datum.address);
						availableCity.push(datum.city);
						availableZipCode.push(datum.zipcode);
						availableMemNumb.push(datum.membershipno);
					}
				})
				$(function() {

					$("#fname").autocomplete({
						source : availableFName
					});
					$("#lname").autocomplete({
						source : availableLName
					});
					$("#address").autocomplete({
						source : availableAddress
					});
					$("#city").autocomplete({
						source : availableCity
					});
					$("#zipCode").autocomplete({
						source : availableZipCode
					});
					$("#memNum").autocomplete({
						source : availableMemNumb
					});
				});

			}
			//alert("Data: " + data + "\nStatus: " + status);
		});

	});
</script>
