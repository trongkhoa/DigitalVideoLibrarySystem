<% include bootstrap.ejs %>
<html>
<head>
<title>Login</title>
</head>
<body>
	<h1 class="text-center">Welcome to Video Digital Library System <br><small lass="text-left">Please log in</small></h1>
	<div class="container">
		<form role="form" action="/validate" method="post">
			<div class="form-group">
				<label for="exampleInputEmail1">User Name</label> <input
					type="text" class="form-control" name="userName" id="uname"
					placeholder="Enter username">
			</div>
			<div class="form-group">
				<label for="exampleInputPassword1">Password</label> <input
					type="password" class="form-control" name="Password"
				id="password"
					placeholder="Password">
			</div>
			
			
			<button type="submit" class="btn btn-default">Submit</button>
		</form>
		
		
	</div>
</body>
</html>